#: ../src/parse.c:11
msgid "Incorrect location format, should be X,Y"
msgstr "位置格式不正確，應爲X,Y"

#: ../src/parse.c:14
msgid "Incorrect size format, should be XxY"
msgstr "大小格式不正確，應爲XxY"

#: ../src/parse.c:17
msgid "Incorrect gravity range, see README for usage"
msgstr "定位原點格式不正確，詳見README文件"

#: ../src/parse.c:20
msgid "Conflicting arguments, see README for usage"
msgstr "參數不相容，詳見README文件"

#: ../src/parse.c:36
msgid "ENUM"
msgstr "原點"

#: ../src/parse.c:37
msgid "Set gravity for window, 1 = Top-left, 9 = Bottom-right"
msgstr "設定視窗定位原點，1代表左上角，9代表右下角"

#: ../src/parse.c:44
msgid "Set the default screen location"
msgstr "設定視窗顯示位置"

#: ../src/parse.c:51
msgid "Set the size"
msgstr "設定視窗大小"

#: ../src/parse.c:57
msgid "TITLE"
msgstr "標題"

#: ../src/parse.c:58
msgid "Set the title for application window"
msgstr "設定視窗標題"

#: ../src/parse.c:64
msgid "PATH"
msgstr "檔案路徑"

#: ../src/parse.c:65
msgid "Set the icon for application window"
msgstr "設定視窗圖示"

#: ../src/parse.c:71
msgid "COMMAND"
msgstr "指令"

#: ../src/parse.c:72
msgid "Set the command to be executed"
msgstr "設定執行的指令"

#: ../src/parse.c:78
msgid "Application can be interacted with mouse and auto focuses"
msgstr "容許以滑鼠操作應用程式和自動轉移焦點"

#: ../src/parse.c:84
msgid "Display as popup instead of a window, implies -1"
msgstr "以彈出式視窗顯示，自動啟用-1模式"

#: ../src/parse.c:90
msgid "Set if only one instance is allowed"
msgstr "設定可否有多個視窗同時執行相同的程式"

#: ../src/parse.c:96
msgid "Toggle single instance application, implies -1"
msgstr "開關程式視窗，自動啟用-1模式"

#: ../src/parse.c:102
msgid "Kill a single instance application according to command"
msgstr "關閉使用-1模式的視窗"

#: ../src/gtkfunc.c:112
msgid "Fork error"
msgstr "分叉失敗"

#: ../src/interface.c:52
msgid "No instance of application is running or it is not ran with -1"
msgstr "找不到執行中的視窗或是該視窗沒有使用-1模式"

#: ../src/interface.c:65
msgid "No such process found, use system kill tool"
msgstr "視窗可能已被強制關閉，請使用系統管理程式或手動關閉程式"

#: ../src/interface.c:73
msgid "PID mismatch in record, use system kill tool"
msgstr "程式執行緒與記錄不同，請使用系統管理程式或手動關閉程式"

#: ../src/interface.c:86
#, c-format
msgid "Existing instance is running, remove -1 flag or '%s' to unlock"
msgstr "程式已在執行中，停用-1模式或移除「%s」以繼續"

#: ../src/interface.c:130
msgid "GTK error"
msgstr "GTK出現錯誤"

#: ../src/libfunc.c:64
msgid "Original process, please end"
msgstr "程式已完結，請結束"

#: ../src/libfunc.c:70
msgid "- simple frontend for shell scripts"
msgstr "- 簡易的命令列視窗介面屬性程式"

#: ../src/libfunc.c:96
msgid "ShellFront integration does not allow cmd option"
msgstr "ShellFront函式庫不容許用家自訂指令（cmd選項）"
